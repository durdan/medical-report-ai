{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "NEXTAUTH_URL": "https://${VERCEL_URL}",
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NEXTAUTH_URL": "https://${VERCEL_URL}",
      "NODE_ENV": "production"
    }
  },
  "functions": {
    "api/**/*.js": {
      "maxDuration": 60
    },
    "app/api/**/*.js": {
      "maxDuration": 60
    }
  }
}
